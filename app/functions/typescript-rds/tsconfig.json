{
    "compilerOptions": {
      "target": "ES2020",
      "outDir": "dist",
      "module": "CommonJS",
      "strict": true,
      "preserveConstEnums": true,
      "sourceMap": false,
      "moduleResolution":"node",
      "esModuleInterop": true, 
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,  
    },
    "include": [ "src/**/*.ts"],
    "exclude": ["node_modules", "**/*.test.ts"]
  }